{
	"name": "@team8/backend",
	"private": true,
	"version": "1.0.0",
	"description": "Nest TypeScript starter repository",
	"license": "MIT",
	"scripts": {
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"start:dev": "NODE_ENV=dev nest start --tsc --watch",
		"start:debug": "NODE_ENV=dev nest start --debug --watch",
		"start:prod": "NODE_ENV=prod node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test:unit": "NODE_ENV=test jest --config ./test/unit/jest.json",
		"test:unit:cov": "NODE_ENV=test jest --config ./test/unit/jest.json --coverage",
		"test:e2e": "NODE_ENV=test jest --config ./test/e2e/jest.json"
	},
	"dependencies": {
		"@nestjs/common": "^10.0.0",
		"@nestjs/config": "^3.1.1",
		"@nestjs/core": "^10.0.0",
		"@nestjs/platform-express": "^10.0.0",
		"@nestjs/serve-static": "^4.0.0",
		"@nestjs/terminus": "^10.2.1",
		"@nestjs/typeorm": "^10.0.1",
		"@team8/constants": "*",
		"@team8/types": "*",
		"@team8/utils": "*",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.1",
		"mysql2": "^3.9.1",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^7.8.1",
		"typeorm": "0.3.19"
	},
	"devDependencies": {
		"@nestjs/cli": "^10.0.1",
		"@nestjs/schematics": "^10.0.1",
		"@nestjs/testing": "^10.0.0",
		"@swc/cli": "^0.1.62",
		"@swc/core": "^1.3.64",
		"@types/express": "^4.17.17",
		"@types/supertest": "^2.0.12",
		"eslint-config-team8": "*",
		"source-map-support": "^0.5.21",
		"supertest": "^6.3.3"
	}
}
