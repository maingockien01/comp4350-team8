{
	"name": "@team8/frontend",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@emotion/react": "^11.11.3",
		"@emotion/styled": "^11.11.0",
		"@mui/icons-material": "^5.15.7",
		"@mui/material": "^5.15.7",
		"@mui/x-charts": "^6.19.3",
		"@team8/constants": "*",
		"npm-watch": "^0.11.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.22.0",
		"vite-plugin-svgr": "^4.2.0",
		"vite-tsconfig-paths": "^4.3.1",
		"web-vitals": "^3.5.1"
	},
	"scripts": {
		"start": "vite",
		"build": "tsc && vite build",
		"build:dev": "tsc && vite build --watch",
		"watch": "npm-watch",
		"lint": "eslint --ext .ts,.tsx src --fix"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@types/react": "^18.2.48",
		"@types/react-dom": "^18.2.18",
		"@types/react-router-dom": "^5.3.3",
		"@vitejs/plugin-react": "^4.2.1",
		"eslint-config-team8": "*",
		"vite": "^5.0.12"
	},
	"watch": {
		"build": {
			"patterns": [
				"src"
			],
			"extensions": "js,jsx,tsx,ts,css"
		}
	}
}
